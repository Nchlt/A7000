main(integer x) (
  var integer x;
  loop(0, x);
)


loop(integer i, integer r) (
  var integer r;
  var integer i;
  if line(point(i, r), r) then (
    loop(( i + 1 ), r);
  ) else (
  );
)


point(integer i, integer r) (
  var integer result;
  var integer r;
  var integer j;
  var integer i;
  j := 0;
  while ( ( ( i * i ) + ( j * j ) ) < ( r * r ) ) (
    j := ( j + 1 );
  );
  result := j;
)


line(integer p, integer r) (
  var boolean result;
  var integer r;
  var integer p;
  var integer j;
  result := false;
  j := 0;
  while ( j < ( r + 1 ) ) (
    if ( j < p ) then (
      print(46);
      result := true;
    ) else (
      print(35);
    );
    print(32);
    j := ( j + 1 );
  );
  print(10);
)


